{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "ts-node": {
      "swc": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  